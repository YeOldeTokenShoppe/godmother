{
  "name": "@coinbase/waas-sdk-web",
  "type": "module",
  "version": "3.3.2",
  "description": "WaaS for web.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint -c .eslintrc.cjs src/**/*.{tsx,ts}",
    "lint-fix": "eslint -c .eslintrc.cjs src/**/*.{tsx,ts} --fix",
    "format": "prettier -c ../.prettierrc --write \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "format-check": "prettier -c ../.prettierrc --check \"**/*.{ts,tsx,js,cjs,json,md}\"",
    "test": "tsc && NODE_OPTIONS=--experimental-vm-modules npx jest --no-cache",
    "test-debug": "tsc && NODE_OPTIONS=--experimental-vm-modules node --inspect-brk node_modules/.bin/jest --runInBand",
    "build-sandbox": "./node_modules/.bin/webpack --config webpack.sandbox.config.cjs",
    "build-sandbox-prod": "node ./scripts/filterSandboxDeps.js && ./node_modules/.bin/webpack --config webpack.sandbox.config.cjs",
    "run-sandbox": "npx webpack serve --config webpack.sandbox.config.cjs",
    "build-version": "node ./scripts/update_version.cjs && node ./scripts/checksum_sandbox.cjs ./src/mpc.tsx",
    "check": "tsc --noEmit",
    "protos": "./scripts/build_protos.sh && ./scripts/move_protos.sh",
    "build-sandbox-rpc": "cd ../utils/sandbox-rpc && npm install && tsc",
    "build": "npm run build-version && tsc && npm run build-sandbox",
    "build-local": "npm run build-version && tsc && DEBUG=true npm run build-sandbox",
    "strip-corekms-wasm": "(rm -f node_modules/@coinbase/corekms/dist/*.wasm || true)",
    "prepack": "npm run build-version && npm run build-sandbox-rpc && tsc && npm run strip-corekms-wasm",
    "prepublishOnly": ""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "waas",
    "web"
  ],
  "author": "Coinbase Inc.",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@jest/globals": "^29.7.0",
    "@types/golang-wasm": "^1.15.0",
    "@types/jest": "^29.5.11",
    "@types/node-jose": "^1.1.10",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "protoc-gen-grpc-web": "^1.4.2",
    "protoc-gen-js": "^3.21.2",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-inject-plugin": "^1.5.5",
    "webpack-shell-plugin-next": "^2.3.1",
    "webpack-subresource-integrity": "^5.2.0-rc.1"
  },
  "dependencies": {
    "@bugsnag/js": "^7.22.3",
    "@coinbase/corekms": "0.1.12",
    "@coinbase/sandbox-rpc": "1.1.6",
    "client-analytics": "0.0.8"
  },
  "sandboxDependencies": [
    "@coinbase",
    "@coinbase/corekms",
    "@coinbase/sandbox-rpc",
    "webpack",
    "webpack-cli",
    "webpack-inject-plugin",
    "webpack-merge",
    "webpack-shell-plugin-next",
    "webpack-sources",
    "html-webpack-plugin",
    "copy-webpack-plugin",
    ".bin",
    "graceful-fs",
    "import-local",
    "resolve-cwd",
    "p-try",
    "path-exists",
    "colorette",
    "interpret",
    "rechoir",
    "resolve",
    "is-core-module",
    "hasown",
    "function-bind",
    "lodash",
    "tapable",
    "schema-utils",
    "serialize-javascript",
    "randombytes",
    "normalize-path",
    "glob-parent",
    "is-glob",
    "is-extglob",
    "fast-glob",
    "micromatch",
    "braces",
    "fill-range",
    "to-regex-range",
    "is-number",
    "picomatch",
    "merge2",
    "@nodelib",
    "run-parallel",
    "queue-microtask",
    "fastq",
    "reusify",
