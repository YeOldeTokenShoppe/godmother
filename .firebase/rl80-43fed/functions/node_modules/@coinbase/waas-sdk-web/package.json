    "babel-plugin-polyfill-corejs3",
    "lodash.debounce"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./config/jest/cryptoPolyfill.cjs",
      "./config/jest/textEncoderPolyfill.cjs"
    ],
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)(\\.js)$": "$1"
    },
    "coveragePathIgnorePatterns": [
      "node_modules",
      "src/generated",
      "__tests__"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testRegex": ".test.ts$"
  },
  "optionalDependencies": {
    "@coinbase/cbpay-js": "^1.10.0"
  }
}
